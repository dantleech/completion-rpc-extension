{
    "name": "phpactor/completion-rpc-extension",
    "description": "RPC support for the Completion Extension",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpactor/completion-extension": "dev-master",
        "phpactor/rpc-extension": "dev-master",
        "phpactor/test-utils": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\CompletionRpc\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\CompletionRpc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Exension\\CompletionRpc\\CompletionRpcExtension"
    }
}
